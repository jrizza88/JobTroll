<h1 class="text-center">Welcome Home {{data.user.firstName}}!</h1>
<div>
<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3 text-center enter_section" id='appform'>
			<form action="/create" method="post">
      {{!-- <form id="appSubmit"> --}}
				<input type="text" class="form-control" name="companyName" value="" id="enter_text" placeholder="Company Name">
                <input type="text" class="form-control" name="position" value="" id="enter_text" placeholder="Position applied for">
                <input type="text" class="form-control" name="dateApplied" value="" id="enter_text" placeholder="mm/dd/yyyyy" >
                <input type="text" class="form-control" name="replied" value="" id="enter_text" placeholder="Have they replied back?">
                <input type="text" class="form-control" name="nextEvent" value="" id="enter_text" placeholder="What is the next event?">
                <input type="text" class="form-control" name="resume" value="" id="enter_text" placeholder="Have you submitted your resume?">
                <input type="text" class="form-control" name="notes" value="" id="enter_text" placeholder="Enter Notes here">
                
				<button type="submit" class="btn btn-primary" id="text-enter-button" name={{user.id}}>Submit</button>
			</form>
			<p></p>
		</div>
	</div>
</div>
</div>

