<h2 class="page-header">Register</h2>
{{#if errors}}
    {{#each errors}}
      <div class="alert alert-danger">{{msg}}</div>
    {{/each}}
{{/if}}
<form method="post" action="/form">
  <div class="form-group">
    <label>First Name</label>
    <input type="Company Name" class="form-control" placeholder="Company Name" name="Company Name">
  </div>
  <div class="form-group">
    <label>Last Name</label>
    <input type="Position" class="form-control" placeholder="Position" name="Position">
  </div>
  <div class="form-group">
    <label>Username</label>
    <input type="Resume" class="form-control" placeholder="Resume" name="Resume">
  </div>
  <div class="form-group">
    <label>Email</label>
    <input type="Date" class="form-control" placeholder="Date Applied" name="Date Applied">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="Reply" class="form-control" placeholder="Replied" name="Replied">
  </div>
  <div class="form-group">
    <label>Confirm Password</label>
    <input type="Next Event" class="form-control" placeholder="Next Event" name="Next Event">
  </div></br>
  <div class="form-group">
    <label>Confirm Password</label>
    <input type="Notes" class="form-control" placeholder="Notes" name="Notes">
  </div></br>
    <button type="submit" class="btn btn-primary" value="Submit">Submit</button>
  </br></br></br>
 </form>
